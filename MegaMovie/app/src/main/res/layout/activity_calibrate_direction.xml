<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="ideum.com.megamovie.Java.NewUI.CalibrateDirectionActivity">

    <fragment
        android:id="@+id/direction_calibration_fragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:name="ideum.com.megamovie.Java.CalibrateDirectionFragment"
        />

    <fragment
        android:id="@+id/preview_fragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:name="ideum.com.megamovie.Java.PatagoniaTest.CameraPreviewFragment"
        />

</LinearLayout>
